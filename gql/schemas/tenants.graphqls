"""
Represents a Tenant entity
"""
type Tenant implements Resource & Taggable {
  """
  User belongs to Account Owner
  """
  accountOwner: User!
  """
  All Client Org Unit belongs to Tenant
  """
  clientOrganizationUnits: [ClientOrganizationUnit!]
  """
  Contact information of the tenant
  """
  contactInfo: ContactInfo
  """
  Timestamp of creation
  """
  createdAt: DateTime!
  """
  Identifier of the user who created the record
  """
  createdBy: UUID!
  """
  Description of the tenant
  """
  description: String
  """
  Unique identifier of the tenant
  """
  id: UUID!
  """
  Name of the tenant
  """
  name: String!
  """
  Status of Tenant
  """
  status: StatusTypeEnum!
  """
  Tags for the account
  """
  tags: [Tags!]
  """
  Type of Resource
  """
  type: String!
  """
  Timestamp of last update
  """
  updatedAt: DateTime!
  """
  Identifier of the user who last updated the record
  """
  updatedBy: UUID!
}

"""
Represents contact information
"""
type ContactInfo {
  """
  Address of the contact
  """
  address: Address
  """
  Email of the contact
  """
  email: String
  """
  Phone number of the contact
  """
  phoneNumber: String
}

"""
Defines input fields for contact information
"""
input ContactInfoInput {
  """
  Address of the contact
  """
  address: AddressInput
  """
  Email of the contact
  """
  email: String
  """
  Phone number of the contact
  """
  phoneNumber: String
}

"""
Defines input fields for creating a tenant
"""
input CreateTenantInput {
  """
  User belongs to Account Owner
  """
  accountOwnerId: UUID!
  """
  Contact information of the tenant
  """
  contactInfo: ContactInfoInput
  """
  Description of the tenant
  """
  description: String
  """
  Unique identifier of the tenant
  """
  id: UUID!
  """
  Name of the tenant
  """
  name: String!
  """
  Associated Tags Input
  """
  tags: [TagInput!]
}

"""
Defines input fields for updating a tenant
"""
input UpdateTenantInput {
  """
  User belongs to Account Owner
  """
  accountOwnerId: UUID
  """
  Updated contact information of the tenant
  """
  contactInfo: ContactInfoInput
  """
  Updated description of the tenant
  """
  description: String
  """
  Unique identifier of the tenant
  """
  id: UUID!
  """
  Updated name of the tenant
  """
  name: String
  """
  Status of Tenant
  """
  status: StatusTypeEnum
  """
  Associated Tags Input
  """
  tags: [TagInput!]
}

"""
Represents an address
"""
type Address {
  """
  City of the address
  """
  city: String
  """
  Country of the address
  """
  country: String
  """
  State of the address
  """
  state: String
  """
  Street of the address
  """
  street: String
  """
  Zip code of the address
  """
  zipcode: String
}

"""
Defines input fields for creating an address
"""
input AddressInput {
  """
  City of the address
  """
  city: String
  """
  Country of the address
  """
  country: String
  """
  State of the address
  """
  state: String
  """
  Street of the address
  """
  street: String
  """
  Zip code of the address
  """
  zipcode: String
}